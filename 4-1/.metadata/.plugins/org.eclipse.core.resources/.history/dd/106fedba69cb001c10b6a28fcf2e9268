#ifndef ALBUMESARTISTASMUSICALES_H_
#define ALBUMESARTISTASMUSICALES_H_

#define AAM_TAM_DESC 51



typedef struct{
	int id;
	char descripcion[AAM_TAM_DESC];
}eDiscografia;


typedef struct{
	int id;
	char descripcion[AAM_TAM_DESC];
}eTipoArtista;


typedef struct{
	int id;
	char descripcion[AAM_TAM_DESC];
}eArtista;



typedef struct{
	int id;
	char descripcion[AAM_TAM_DESC];
	eAaaaMmDd fechaPublicacion;
	float importe;

	int estado;
}eAlbum;







#endif /* ALBUMESARTISTASMUSICALES_H_ */
